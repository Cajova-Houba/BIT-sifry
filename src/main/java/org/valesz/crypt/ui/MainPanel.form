<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="org.valesz.crypt.ui.MainPanel">
  <grid id="27dc6" binding="mainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <tabbedpane id="3e092" binding="standardCiphersTab">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="aa6fc" binding="byHandPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints>
              <tabbedpane title="By hand"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <grid id="73746" binding="configPanel" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="0" left="0" bottom="0" right="0"/>
                <constraints border-constraint="East"/>
                <properties/>
                <border type="none"/>
                <children/>
              </grid>
              <grid id="abf88" binding="inputPanel" custom-create="true" layout-manager="GridBagLayout">
                <constraints border-constraint="Center"/>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="f9f23" class="javax.swing.JLabel" binding="inputTexLabel">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                      <gridbag weightx="0.0" weighty="0.0"/>
                    </constraints>
                    <properties>
                      <text value="Encrypted message:"/>
                    </properties>
                  </component>
                  <component id="e5a5a" class="javax.swing.JTextArea" binding="inputTextArea">
                    <constraints>
                      <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
                        <preferred-size width="150" height="50"/>
                      </grid>
                      <gridbag weightx="0.0" weighty="0.0"/>
                    </constraints>
                    <properties>
                      <columns value="50"/>
                      <font name="Courier New"/>
                      <rows value="10"/>
                    </properties>
                  </component>
                </children>
              </grid>
            </children>
          </grid>
          <grid id="3434a" binding="standardCiphersPanel" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints>
              <tabbedpane title="Standard ciphers"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children/>
          </grid>
        </children>
      </tabbedpane>
    </children>
  </grid>
</form>
